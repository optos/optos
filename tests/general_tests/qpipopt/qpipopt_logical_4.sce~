// A simple example without constraints
A= [];
conLB=[];
conUB = [];
lb=repmat(0,3,1);
ub=repmat(3,3,1);
f=[2 -35 -47]'; 
H =[5   -2   -1; -2   4   3; -1   3   5];
nbVar = 3;
nbCon = 0;


//Output
